{
  "name": "reactizy",
  "version": "5.0.0",
  "description": "Reactizy is a React High Order Component and a toolkit allowing to simplify use of react-redux, providing asynchronous reducers, permitting to split *react components* in order to have a better readability, call api with simply library and some usefull utilities.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://pbenard73.github.io/reactizy/",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbenard73/reactizy"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "rm -rf ./dist; ./node_modules/webpack/bin/webpack.js && npx tsc",
    "doc": "react-scripts build",
    "test": "jest",
    "lint": "node_modules/eslint/bin/eslint.js --fix src/*",
    "start": "react-scripts start"
  },
  "keywords": [
    "react",
    "toolkit",
    "redux",
    "redux async",
    "react-redux",
    "redux-thunk",
    "reducer",
    "architecture",
    "simplify",
    "easy"
  ],
  "author": "BENARD Patrick <pbenard73@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "express": "^4.17.1",
    "node-sass": "^4.14.1",
    "react": ">16.13.1",
    "react-awesome-button": "^6.5.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "react-syntax-highlighter": "^13.4.0",
    "source-map-explorer": "^2.5.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.1",
    "prop-types": ">=15.7.2",
    "react-dom": ">=16.13.1",
    "react-redux": ">=7.2.1",
    "redux": ">=4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
